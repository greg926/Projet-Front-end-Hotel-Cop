!function($){var a="//d1uq38jsc65f9d.cloudfront.net/",e=$("[data-toast]");e.length>0&&$(window).load(function(){e.each(function(){var e=$(this);e.addClass("c-toast");var s=e.data("toast").toLowerCase(),t="";switch(className="",imgFilename="",s){case"qtrback":case"quarterback":t="Quarterback",className="c-toast--quarterback",imgFilename="quarterback-logo-2015.png";break;case"journey":t="Journey",className="c-toast--journey",imgFilename="journey-logo-2015.png"}e.addClass(className),e.append('<span class="c-toast__tooltip"><img class="c-toast__image" src="'+a+"img/"+imgFilename+'" alt="'+t+' logo"></span>'),$("head").append('<link rel="stylesheet" href="'+a+'toast.css">')})})}(jQuery);